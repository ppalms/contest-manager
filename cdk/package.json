    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@aws-appsync/utils": "^1.3.1",
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.93.0-alpha.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.410.0",
    "@aws-sdk/client-dynamodb": "^3.410.0",
